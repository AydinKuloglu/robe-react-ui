{
  "rootDir": "../../",
  "automock": false,
  "bail": false,
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "testRegex": "/__test\/.*\\.spec\\.js$",
  "testRunner": "jasmine2",
  "setupTestFrameworkScriptFile": "<rootDir>/config/test/setup.js",
  "collectCoverage": false,
  "moduleNameMapper": {
    "^robe-react-ui\\/lib\\/(.*)$" : "<rootDir>/src/$1",
    "^robe-react-ui$" : "<rootDir>/src/index"
  },
  "modulePaths": [
    "src"
  ],
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "moduleDirectories": [
    "node_modules",
    "bower_components"
  ]
}